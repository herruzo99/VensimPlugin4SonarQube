"Temp change 1.5C"=
	1.5
	~	DegreesC
	~	Temperature change of 1.5C as reference.
	|

HadCRUT4 Temperature:INTERPOLATE::=
	GET XLS DATA('inputs_W.xlsx', 'Climate', '7', 'O8')
	~	DegreesC
	~	HadCRUT4 Temperature Data
		https://www.metoffice.gov.uk/hadobs/hadcrut4/data/current/download.html
	|

CO2 Mauna Loa:INTERPOLATE::=
	GET XLS DATA('inputs_W.xlsx', 'Climate', '3', 'O4')
	~	ppm
	~	Mauna Loa CO2 annual mean data. \
		(https://www.esrl.noaa.gov/gmd/ccgg/trends/data.html)
	|

GISS NASA Temperature:INTERPOLATE::=
	GET XLS DATA('inputs_W.xlsx', 'Climate', '7', 'AS9')
	~	DegreesC
	~	GISS NASA Temperature.https://data.giss.nasa.gov/gistemp/graphs/
	|

density=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C43')
	~	kg/meter/meter/meter
	~	Density of water, i.e., mass per volume of water.
	|

sec per day=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'K9')
	~	s/day
	~	86400 (60*60*24) seconds per day
	|

sec per yr= INITIAL(
	days per year*sec per day)
	~	s/Year
	~	Converts seconds to years
	|

init Atmos UOcean Temp=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C38')
	~	DegreesC
	~	Global Annual Temperature Anomaly (Land + Ocean) in 1990 from NASA GISS Surface \
		Temperature (GISTEMP): +0.43 ºC.
		5-year average: +0.36 ºC. Average 1880-1889 = -0,225. Preindustrial reference: 0,36 \
		+ 0,225 = 0,585
		http://cdiac.ornl.gov/ftp/trends/temp/hansen/gl_land_ocean.txt
	|

watt per J s=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'K7')
	~	watt/(J/s)
	~	Converts watts to J/s.
	|

lower layer volume Vu[Layers]= INITIAL(
	area*(1-land area fraction)*Layer Depth[Layers])
	~	meter*meter*meter
	~	Water equivalent volume of the deep ocean by layer.
	|

volumetric heat capacity= INITIAL(
	mass heat cap*watt per J s/sec per yr*density)
	~	watt*Year/meter/meter/meter/DegreesC
	~	Volumetric heat capacity of water, i.e., amount of heat in watt*year \
		required to raise 1 cubic meter of water by one degree C.
	|

mass heat cap=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C42')
	~	J/kg/DegreesC
	~	Specific heat of water, i.e., amount of heat in Joules per kg water \
		required to raise the temperature by one degree Celsius.
	|

Heat Diffusion Covar=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C35')
	~	Dmnl
	~	Fraction of heat transfer that depends on eddy diffusion.
	|

"2x CO2 Forcing"= INITIAL(
	CO2 Rad Force*LN(2))
	~	watt/meter/meter
	~		|

Climate Sensitivity to 2x CO2[BAU]=
	GET XLS CONSTANTS('inputs_W.xlsx', 'BAU', 'C116') ~~|
Climate Sensitivity to 2x CO2[SCEN1]=
	GET XLS CONSTANTS('inputs_W.xlsx', 'SCEN1', 'C116') ~~|
Climate Sensitivity to 2x CO2[SCEN2]=
	GET XLS CONSTANTS('inputs_W.xlsx', 'SCEN2', 'C116') ~~|
Climate Sensitivity to 2x CO2[SCEN3]=
	GET XLS CONSTANTS('inputs_W.xlsx', 'SCEN3', 'C116') ~~|
Climate Sensitivity to 2x CO2[SCEN4]=
	GET XLS CONSTANTS('inputs_W.xlsx', 'SCEN4', 'C116') ~~|
Climate Sensitivity to 2x CO2[User defined]=
	GET XLS CONSTANTS('inputs_W.xlsx', 'User defined', 'C116')
	~	DegreesC
	~	[Fiddaman] Equilibrium temperature change in response to a 2xCO2 equivalent change \
		in radiative forcing. /2.908 /.
		[DICE-2013R] t2xco2 Equilibrium temp impact (ºC per doubling CO2) /2.9 /
	|

Temperature change [scenarios]=
	Heat in Atmosphere and Upper Ocean[scenarios]/Atm and Upper Ocean Heat Cap
	~	DegreesC
	~	Temperature of the Atmosphere and Upper Ocean, relative to preindustrial \
		reference period
	|

land thickness=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C40')
	~	meter
	~	Effective land area heat capacity, expressed as equivalent water layer \
		thickness.
	|

Heat Transfer[scenarios,Layer1]=
	(Temperature change[scenarios]-Relative Deep Ocean Temp[scenarios,Layer1])*Heat Transfer Coeff\
		/Mean Depth of Adjacent Layers
	[Layer1] ~~|
Heat Transfer[scenarios,lower]=
	(Relative Deep Ocean Temp[scenarios,upper]-Relative Deep Ocean Temp[scenarios,lower]\
		)
	*Heat Transfer Coeff/Mean Depth of Adjacent Layers[lower]
	~	watt/meter/meter
	~	Heat Transfer from the Atmosphere & Upper Ocean to the Deep Ocean
	|

Climate Feedback Param[scenarios]= INITIAL(
	"2x CO2 Forcing"/Climate Sensitivity to 2x CO2[scenarios])
	~	(watt/meter/meter)/DegreesC
	~	Climate Feedback Parameter - determines feedback effect from temperature \
		increase.
	|

area=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C39')
	~	meter*meter
	~	Global surface area.
	|

Heat Transfer Coeff= INITIAL(
	(Heat Transfer Rate*Mean Depth of Adjacent Layers[Layer1])
	*(Heat Diffusion Covar*(Eddy diff coeff/Eddy diff mean)+(1-Heat Diffusion Covar)))
	~	watt/(meter*meter)/(DegreesC/meter) [0,1]
	~	The ratio of the actual to the mean of the heat transfer coefficient, \
		which controls the movement of heat through the climate sector, is a \
		function of the ratio of the actual to the mean of the eddy diffusion \
		coefficient, which controls the movement of carbon through the deep ocean.
	|

Eddy diff coeff= INITIAL(
	Eddy diff coeff index*Eddy diff mean)
	~	meter*meter/Year
	~	Multiplier of eddy diffusion coefficient mean
	|

upper layer volume Vu= INITIAL(
	area*(land area fraction*land thickness+(1-land area fraction)*Mixed Depth))
	~	meter*meter*meter
	~	Water equivalent volume of the upper box, which is a weighted combination \
		of land, atmosphere,and upper ocean volumes.
	|

Heat in Deep Ocean[scenarios,upper]= INTEG (
	Heat Transfer[scenarios,upper]-Heat Transfer[scenarios,lower],
		Init Deep Ocean Temp[upper]*Deep Ocean Heat Cap[upper]) ~~|
Heat in Deep Ocean[scenarios,bottom]= INTEG (
	Heat Transfer[scenarios,bottom],
		Init Deep Ocean Temp[bottom]*Deep Ocean Heat Cap[bottom])
	~	watt*Year/(meter*meter)
	~	Heat content of each layer of the deep ocean.
	|

land area fraction=
	GET XLS CONSTANTS('inputs_W.xlsx', 'Climate', 'C41')
	~	fraction
	~	Fraction of global surface area that is land.
	|

