extraction conv gas EJ[scenarios]=
	IF THEN ELSE(RURR conv gas[scenarios]<0,0,
	IF THEN ELSE("unlimited NRE?"[scenarios]=1,Demand conv gas[scenarios],
	IF THEN ELSE("unlimited gas?"[scenarios]=1, Demand conv gas[scenarios],
	MIN(Demand conv gas[scenarios], max extraction conv gas EJ[scenarios]))))
	~	EJ/Year
	~	Annual extraction of conventional gas.
	|

check gases[scenarios]=
	ZIDZ( PED gases[scenarios]-PES gases[scenarios] , PES gases[scenarios] )
	~	Dmnl
	~	Variable to avoid energy oversupply caused by exogenously driven policies.
	|

abundance gases[scenarios]=
	IF THEN ELSE(PED gases[scenarios]<PES gases[scenarios], 1, 1-ZIDZ( PED gases[scenarios\
		]-PES gases[scenarios] , PED gases[scenarios] ))
	~	Dmnl
	~	The parameter abundance varies between (1;0). Abundance=1 while the supply \
		covers the demand; the closest to 0 indicates a higher divergence between \
		supply and demand.
	|

Demand conv gas[scenarios]=
	MAX("PED nat. gas EJ"[scenarios]-extraction unconv gas EJ[scenarios], 0)
