Final energy intensity 2020[scenarios,final sources,sectors]=
	IF THEN ELSE(Time<year energy intensity target[scenarios],Evol final energy intensity by sector and fuel
	[scenarios,sectors,final sources],aux20[scenarios
	,final sources,sectors])
	~	EJ/Tdollars
	~	Energy intensity by sector and final source in 2009
	|

variation energy intensity TARGET[scenarios,sectors,final sources]=
	IF THEN ELSE(Choose energy intensity target method[scenarios]=1,IF THEN ELSE(Time>final year target\
		[scenarios],0,IF THEN ELSE
	(Time
	<year energy intensity target[scenarios],0,((Energy intensity target by sector and fuel\
		[scenarios,sectors,final sources
	]-Final energy intensity 2020
	[scenarios,final sources,sectors])/(final year target[scenarios]-year energy intensity target\
		[scenarios])))),IF THEN ELSE
	(Time>final year target[scenarios
	],0,IF THEN ELSE(Time
	<year energy intensity target[scenarios],0,((Final energy intensity 2020[scenarios,final sources\
		,sectors]*(1+pct change energy intensity target
	[scenarios])-Final energy intensity 2020
	[scenarios,final sources,sectors])/(final year target[scenarios]-year energy intensity target\
		[scenarios])))))
	~	EJ/Tdollars
	~	Variation in energy intensity by sector and final energy defined by user \
		targets.
	|

scarcity primary sources[scenarios,primary sources]=
	1-Abundance primary sources[scenarios,primary sources]
	~	Dmnl
	~	The parameter scarcity varies between (1;0). (Scarcity =1-Abundance)
		Scarcity=0 while the supply covers the demand; the closest to 1 indicates \
		a higher divergence between supply and demand.
	|

increase in perception PS scarcity[scenarios,primary sources]=
	scarcity primary sources[scenarios,primary sources]*sensitivity to scarcity[scenarios\
		]*(1-perception in primary sources scarcity[scenarios,primary sources])
	~	Dmnl
	~	Increase in socieconomic perception of primary sources scarcity of each \
		fuel
	|

Abundance primary sources[scenarios,coal]=
	abundance coal[scenarios] ~~|
Abundance primary sources[scenarios,oil]=
	abundance total oil[scenarios] ~~|
Abundance primary sources[scenarios,natural gas]=
	"abundance total nat. gas"[scenarios] ~~|
Abundance primary sources[scenarios,others]=
	1
	~	Dmnl
	~	The parameter abundance varies between (1;0). Abundance=1 while the supply \
		covers the demand; the closest to 0 indicates a higher divergence between \
		supply and demand.
	|

perception in primary sources scarcity[scenarios,primary sources]= INTEG (
	increase in perception PS scarcity[scenarios,primary sources]-reduction in perception PS scarcity\
		[scenarios,primary sources],
		0)
	~	Dmnl
	~	Perception of primary sources scarcity of each fuel by economic sectors. \
		This perception drives the fuel replacement for electriciy and heat.
	|

reduction in perception PS scarcity[scenarios,primary sources]=
	perception in primary sources scarcity[scenarios,primary sources]/energy scarcity forgetting time\
		[scenarios]
	~	Dmnl
	~	Reduction of the perception of energy scarcity of economic sectors due to \
		the "forgetting" effect.
	|

"perception of inter-fuel primary sources scarcity"[scenarios,coal,primary sources]=
	IF THEN ELSE(sensitivity to scarcity[scenarios]=0,0,ZIDZ( perception in primary sources scarcity\
		[scenarios,primary sources]-perception in primary sources scarcity[scenarios,coal],\
		 1)) ~~|
"perception of inter-fuel primary sources scarcity"[scenarios,oil,primary sources]=
	IF THEN ELSE(sensitivity to scarcity[scenarios]=0,0,ZIDZ( perception in primary sources scarcity\
